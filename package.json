{
  "name": "express-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "pm2 start ./dist/src/index.js",
    "dev": "npm run db:migrate && set DEBUG=http,mail,express:* & nodemon ./src/index.ts",
    "build": "tsc -t es5",
    "lint": "eslint --quiet --ext=ts src",
    "lint:fix": "eslint --fix --quiet --ext=ts src",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "db:migrate": "npm run build:migrations && sequelize db:migrate",
    "db:migrate:qa": "npm run build:migrations && NODE_ENV=qa sequelize db:migrate",
    "db:migrate:production": "NODE_ENV=production sequelize db:migrate",
    "migration:generate": "sequelize migration:generate --migrations-path src/migrations --name",
    "seed:generate": "sequelize seed:generate --seeders-path src/seeders --name",
    "db:seed": "npm run build:migrations && sequelize db:seed:all",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^7.1.0",
    "@socket.io/redis-emitter": "^4.1.1",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "debug": "~2.6.9",
    "dotenv": "^14.1.0",
    "express": "~4.16.1",
    "express-validation": "^3.0.8",
    "express-winston": "^4.2.0",
    "helmet": "^5.0.2",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "morgan": "~1.9.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pm2": "^5.2.0",
    "redis": "^4.0.2",
    "sequelize": "^6.13.0",
    "sequelize-cli": "^6.4.1",
    "socket.io": "^4.4.1",
    "util": "^0.12.4",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mkdirp": "^1.0.2",
    "@types/morgan": "^1.9.3",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
